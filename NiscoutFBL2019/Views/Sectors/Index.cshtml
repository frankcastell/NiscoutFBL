@model IEnumerable<NiscoutFBL2019.Models.Sector>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Cod_Sector)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nombre_Sector)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Municipio.Cod_Municipio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Distrito.Cod_Distrito)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Cod_Sector)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre_Sector)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Municipio.Cod_Municipio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Distrito.Cod_Distrito)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>

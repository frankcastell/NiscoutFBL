                        <form action="~/VR_LDE_FT_01/Informe" method="get" id="frmReporte">
                            <input type="hidden" value="fecha" />
                            <button class=" btn btn-primary" type="submit" id="btnReport">Generar Reporte<i class="glyphicon glyphicon-save-file"></i> </button>

                            <input style="visibility:hidden" type="text" class="form-control" name="fecha" id="fecha" value="@ViewBag.Fecha" readonly />
                        </form>

                    </div>
                    <!-- /.info-box-content -->
                    <!-- /.info-box -->

                </div>


            </div>

        </div>
</body>
